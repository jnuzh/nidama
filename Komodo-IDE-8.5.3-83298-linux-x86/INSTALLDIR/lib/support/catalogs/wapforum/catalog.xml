PUBLIC "-//WAPFORUM//DTD WML 1.0//EN" "wml.xml"

-->
    <public publicId="-//WAPFORUM//DTD WML 1.3//EN"
            uri="wml13.dtd"/>
    <public publicId="-//WAPFORUM//DTD WML 1.2//EN"
            uri="wml12.dtd"/>
    <public publicId="-//WAPFORUM//DTD WML 1.1//EN"
            uri="wml_1.1.xml"/>
    <public publicId="-//WAPFORUM//DTD WML 1.0//EN"
            uri="wml.xml"/>

